{
  "compilerOptions": {
    "target": "ES6",
    "module": "ESNext",
    "lib": ["dom", "es2015"],
    "moduleResolution": "bundler",
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "outDir": "./dist",
    "skipLibCheck": false,
    "types": [
      "unplugin-icons/types/vue",
      "vite/client",
      "chrome",
    ],
  },
  "include": ["src/**/*", "vite.config.ts", "src/vite-env.d.ts"],
  "exclude": ["node_modules", "dist"]
}
